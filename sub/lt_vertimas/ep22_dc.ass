Dialogue: 0,0:25:03.81,0:25:05.02,,IBUKI (OFF),0,0,0,,Eight...{  \x  Eight...    }
Dialogue: 0,0:25:05.02,0:25:06.15,,IBUKI (OFF),0,0,0,,Seven...{  \x  Seven...    }
Dialogue: 0,0:25:06.15,0:25:07.40,,IBUKI (OFF),0,0,0,,Six...{  \x  Six...    }
Dialogue: 0,0:25:07.40,0:25:08.65,,IBUKI (OFF),0,0,0,,Five...{  \x  Five...    }
Dialogue: 0,0:25:08.65,0:25:09.86,,IBUKI (OFF),0,0,0,,Four...{  \x  Four...    }
Dialogue: 0,0:25:09.86,0:25:11.11,,IBUKI (OFF),0,0,0,,Three...{  \x  Three...    }
Dialogue: 0,0:25:11.11,0:25:12.03,,IBUKI (OFF),0,0,0,,Two...{  \x  Two...    }
