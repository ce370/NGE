Dialogue: 0,0:01:30.40,0:01:34.11,,TEXT,0,0,0,,NERV Headquarters - Test Facility 2 22 Days Ago{  \x  NERV Headquarters - Test Facility 2 22 Days Ago    }
Dialogue: 0,0:01:33.14,0:01:34.92,,IKARI,0,0,0,,Commence activation.{  \x  Commence activation.    }
Dialogue: 0,0:01:34.92,0:01:36.79,,RITSUKO (OFF),0,0,0,,Connecting main power to all circuits.{  \x  Connecting main power to all circuits.    }
Dialogue: 0,0:01:36.79,0:01:36.79,,IBUKI,0,0,0,,{ ★ }Power supply connected.{  \x  Power supply connected.    }
Dialogue: 0,0:01:36.79,0:01:40.07,,IBUKI,0,0,0,,Commencing activation system.{  \x  Commencing activation system.    }{  ☀ SEPHIROTIC\x    Main power supply connected. Commencing activation system.    }
Dialogue: 0,0:01:43.49,0:01:48.74,,IBUKI,0,0,0,,Activation voltage is approaching critical level. 0.5, 0.2. Rising.{  \x  Activation voltage is approaching critical level. 0.5, 0.2. Rising.    }
Dialogue: 0,0:01:48.74,0:01:51.04,,RITSUKO (OFF),0,0,0,,Begin the second phase of the activation system.{  \x  Begin the second phase of the activation system.    }
Dialogue: 0,0:01:51.04,0:01:53.01,,MAN A (OFF),0,0,0,,Pilot has begun link-up.{  \x  Pilot has begun link-up.    }
Dialogue: 0,0:01:53.01,0:01:54.72,,IBUKI(OFF),0,0,0,,Start system Phase 2.{  \x  Start system Phase 2.    }
Dialogue: 0,0:01:56.59,0:01:58.95,,MAN B (OFF),0,0,0,,Synapses inserted. Junctions connecting.{  \x  Synapses inserted. Junctions connecting.    }
Dialogue: 0,0:01:58.95,0:02:00.22,,WOMAN A (OFF),0,0,0,,Transmitting pulse.{  \x  Transmitting pulse.    }
Dialogue: 0,0:02:00.22,0:02:01.82,,MAN A (OFF),0,0,0,,All circuits are operational.{  \x  All circuits are operational.    }
Dialogue: 0,0:02:01.82,0:02:03.89,,IBUKI (OFF),0,0,0,,No problems with initial contact.{  \x  No problems with initial contact.    }
Dialogue: 0,0:02:03.89,0:02:06.63,,MAN B (OFF),0,0,0,,Power supplied to brachial muscles in both arms.{  \x  Power supplied to brachial muscles in both arms.    }
Dialogue: 0,0:02:07.90,0:02:09.83,,MAN A (OFF),0,0,0,,All nerve links checking out normal.{  \x  All nerve links checking out normal.    }
Dialogue: 0,0:02:09.83,0:02:12.27,,IBUKI (OFF),0,0,0,,Check list satisfactory up to 2550.{  \x  Check list satisfactory up to 2550.    }
Dialogue: 0,0:02:12.27,0:02:13.56,,RITSUKO (OFF),0,0,0,,Prepare for third stage connection.{  \x  Prepare for third stage connection.    }
Dialogue: 0,0:02:13.56,0:02:15.86,,IBUKI (OFF),0,0,0,,Clear up to 2580.{  \x  Clear up to 2580.    }
Dialogue: 0,0:02:15.86,0:02:18.10,,IBUKI (OFF),0,0,0,,Counting down to the absolute borderline.{  \x  Counting down to the absolute borderline.    }
Dialogue: 0,0:02:18.10,0:02:19.43,,IBUKI (OFF),0,0,0,,0.9...{  \x  0.9...    }
Dialogue: 0,0:02:19.43,0:02:20.64,,IBUKI (OFF),0,0,0,,0.7...{  \x  0.7...    }
Dialogue: 0,0:02:20.64,0:02:21.97,,IBUKI (OFF),0,0,0,,0.5...{  \x  0.5...    }
Dialogue: 0,0:02:21.97,0:02:23.24,,IBUKI (OFF),0,0,0,,0.4...{  \x  0.4...    }
Dialogue: 0,0:02:23.24,0:02:24.51,,IBUKI (OFF),0,0,0,,0.3...{  \x  0.3...    }
Dialogue: 0,0:02:24.51,0:02:25.99,,IBUKI (OFF),0,0,0,,The pulses are flowing backward!{  \x  The pulses are flowing backward!    }
Dialogue: 0,0:02:28.04,0:02:29.86,,MAN B (OFF),0,0,0,,Something's going wrong in the third stage!{  \x  Something's going wrong in the third stage!    }
Dialogue: 0,0:02:30.76,0:02:33.13,,AOBA (OFF),0,0,0,,Rejection occurring in the nerve center elements!{  \x  Rejection occurring in the nerve center elements!    }
Dialogue: 0,0:02:35.96,0:02:39.19,,RITSUKO,0,0,0,,Cease contact! Break all circuits through number six!{  \x  Cease contact! Break all circuits through number six!    }
Dialogue: 0,0:02:39.19,0:02:41.50,,IBUKI,0,0,0,,I can't! The signal's not being received!{  \x  I can't! The signal's not being received!    }
Dialogue: 0,0:02:53.82,0:02:56.14,,IBUKI (OFF),0,0,0,,Unit 00 is out of control!{  \x  Unit 00 is out of control!    }
Dialogue: 0,0:02:56.14,0:02:58.54,,IKARI,0,0,0,,Abort the experiment. Shut off the power supply.{  \x  Abort the experiment. Shut off the power supply.    }
Dialogue: 0,0:02:58.54,0:02:59.65,,RITSUKO (OFF),0,0,0,,Yes, Sir!{  \x  Yes, Sir!    }
Dialogue: 0,0:03:08.41,0:03:10.55,,RITSUKO,0,0,0,,Unit 00 has switched to backup power!{  \x  Unit 00 has switched to backup power!    }
Dialogue: 0,0:03:10.55,0:03:12.54,,AOBA (OFF),0,0,0,,Thirty-five seconds until it comes to a complete stop.{  \x  Thirty-five seconds until it comes to a complete stop.    }
Dialogue: 0,0:03:22.38,0:03:23.55,,RITSUKO,0,0,0,,It's dangerous! Please get back!{  \x  It's dangerous! Please get back!    }
Dialogue: 0,0:03:26.28,0:03:28.27,,IBUKI,0,0,0,,Auto ejection activating!{  \x  Auto ejection activating!    }
Dialogue: 0,0:03:28.27,0:03:28.56,,IKARI,0,0,0,,No!{  \x  No!    }
Dialogue: 0,0:03:34.62,0:03:35.68,,IBUKI (OFF),0,0,0,,Complete shutdown in ten seconds. 9...{  \x  Complete shutdown in ten seconds. 9...    }
Dialogue: 0,0:03:35.68,0:03:37.38,,RITSUKO,0,0,0,,Use the special Bakelite! Hurry!{  \x  Use the special Bakelite! Hurry!    }{  ☀ SEPHIROTIC\x    Use the special Bakelite! Hurry! Complete shutdown in ten seconds. 9...    }
Dialogue: 0,0:03:37.38,0:03:37.55,,IBUKI (OFF),0,0,0,,8...{  \x  8...    }{  ☀ SEPHIROTIC\x    Use the special Bakelite! Hurry! 8...    }
Comment: 0,0:03:37.55,0:03:38.35,,,0,0,0,,{  ☀ SEPHIROTIC\x    8...    }
Dialogue: 0,0:03:38.35,0:03:39.28,,IBUKI (OFF),0,0,0,,7...{  \x  7...    }
Dialogue: 0,0:03:39.28,0:03:39.41,,IKARI,0,0,0,,Rei!{  \x  Rei!    }{  ☀ SEPHIROTIC\x    Rei! 7...    }
